  },
  "homepage": "https://github.com/micael95/bdev_ts_api#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.5",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.12",
    "@types/express-serve-static-core": "^4.17.22",
    "@types/faker": "^5.5.6",
    "@types/graphql": "^14.5.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/mongodb": "^3.6.19",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.6.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "apollo-server-integration-testing": "^3.0.0",
    "bson-objectid": "^2.0.1",
    "copyfiles": "^2.4.1",
