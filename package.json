    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.2.6",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "mockdate": "^3.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
